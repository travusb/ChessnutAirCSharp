<Application x:Class="ChessnutBoardEmulator.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:viewModels="clr-namespace:ChessnutBoardEmulator.ViewModels"
             xmlns:views="clr-namespace:ChessnutBoardEmulator.Views"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
         <DataTemplate DataType="{x:Type viewModels:BoardViewModel}">
             <views:BoardView></views:BoardView>
         </DataTemplate>
    </Application.Resources>
</Application>
